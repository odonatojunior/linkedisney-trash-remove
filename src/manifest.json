{
  "manifest_version": 3,
  "name": "Linkedisney Trash Remove",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["tabs", "webNavigation"],
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/**"],
      "js": ["content.ts"]
    }
  ],
  "icons": {
    "16": "images/linkedisney-16.png",
    "32": "images/linkedisney-32.png",
    "48": "images/linkedisney-48.png",
    "128": "images/linkedisney-128.png"
  }
}
